<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane prefHeight="656.0" prefWidth="556.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <Label layoutX="40.0" layoutY="18.0" text="URL">
      <font>
         <Font size="14.0" />
      </font></Label>
   <TextField fx:id="urlName" layoutX="68.0" layoutY="15.0" prefHeight="25.0" prefWidth="390.0" onKeyPressed="#enterAnalyze" text = " http://msoe.us/taylor/cs1021/Lab6 "/>
    <Button layoutX="458.0" layoutY="15.0" onAction="#analyze" prefHeight="25.0" prefWidth="83.0" text="Analyze" />
    <Label layoutX="40.0" layoutY="45.0" text="Size">
      <font>
         <Font size="14.0" />
      </font></Label>
    <TextField fx:id="size" editable="false" layoutX="69.0" layoutY="42.0" prefHeight="25.0" prefWidth="140.0" />
    <Label layoutX="253.0" layoutY="45.0" text="Download Time">
      <font>
         <Font size="14.0" />
      </font></Label>
    <TextField fx:id="downloadTime" editable="false" layoutX="352.0" layoutY="42.0" prefHeight="25.0" prefWidth="190.0" />
    <Label layoutX="40.0" layoutY="71.0" text="Port">
      <font>
         <Font size="14.0" />
      </font></Label>
    <TextField fx:id="port" editable="false" layoutX="69.0" layoutY="68.0" prefHeight="25.0" prefWidth="140.0" />
    <Label layoutX="321.0" layoutY="71.0" text="Host">
      <font>
         <Font size="14.0" />
      </font></Label>
    <TextField fx:id="host" editable="false" layoutX="352.0" layoutY="68.0" prefHeight="25.0" prefWidth="190.0" />
    <Label layoutX="15.0" layoutY="96.0" text="Timeout">
      <font>
         <Font size="14.0" />
      </font></Label>
    <TextField fx:id="timeout" layoutX="69.0" layoutY="93.0" prefHeight="25.0" prefWidth="390.0" onKeyPressed="#enterSet" />
    <Button layoutX="459.0" layoutY="93.0" onAction="#setTimeout" prefHeight="25.0" prefWidth="83.0" text="Set" />
   <TextArea fx:id="content" editable="false" layoutX="15.0" layoutY="128.0" prefHeight="511.0" prefWidth="528.0" wrapText="true" />
</Pane>
